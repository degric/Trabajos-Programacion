Algoritmo OperacionesNumeros
	Escribir  "Primer Numero: ";
	Leer  num1;
	Escribir  "Segundo Numero; ";
	Leer num2;
	sum <- num1 + num2;
	res<- num1 - num2;
	mul <- num1* num2;
	div <- num1 / num2;
	pot <- num1^num2;
	raz <- num1^(1/num2);
	
	Escribir  "La suma es; ", sum;
	Escribir  "La resta es; ", res;
	Escribir  "La multiplicacion es; ", mul;
	Escribir  "La division es; ", div;
	Escribir  "La potencia es; ", pot;
	Escribir  "La raiz es; ", raz;
	
FinAlgoritmo
