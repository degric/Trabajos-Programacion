Algoritmo Calificacion
	Escribir  "Ingresa la calificacion del segundo parcial: ";
	Leer  par1;
	Escribir  "Ingresa la calificacion del segundo parcial: ";
	Leer  par2;
	Escribir  "Ingresa la calificacion del tercer parcial: ";
	Leer par3;
	Escribir  "Ingresa la calificacion del cuarto parcial: ";
	
	Leer par4;
	cal <- (par1 + par2 + par3 + par4)/4;
	Escribir  "La calificacion es: ", cal;
FinAlgoritmo
